<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="ThemeToolBar">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">#404040</item>
        <item name="colorPrimary">#404040</item>
        <item name="android:textColorSecondary">@color/white</item>
    </style>
    <style name="MyToolbarMenuStyle">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@color/black</item>
    </style>
    <style name="SearchAutoCompleteTextView" parent="Widget.AppCompat.AutoCompleteTextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
    </style>
    <style name="recyclerViewStyle" parent="Widget.AppCompat.ListView">
        <item name="android:background">#242424</item>
    </style>
    <style name="cardViewStyle" parent="CardView.Dark">
        <item name="android:textColor">#CCCCAF</item>
        <item name="android:background">#000000</item>
    </style>
</resources>