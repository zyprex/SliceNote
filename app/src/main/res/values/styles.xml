<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="ThemeToolBar">
        <item name="android:textColor">#FFF</item>
        <item name="android:background">#4D3900</item>
        <item name="colorPrimary">#4D3900</item>
        <item name="android:textColorSecondary">#FFF</item>
        <!-- for SearchView style -->
        <item name="android:textColorPrimary">#FFF</item>
        <item name="colorControlNormal">#FFF</item>
        <item name="colorControlHighlight">#F00</item>
        <item name="android:textColorHint">#888</item>
    </style>
    <style name="MyToolbarMenuStyle">
        <item name="android:textColor">#000</item>
        <item name="android:background">#FFF</item>
    </style>
    <style name="SearchAutoCompleteTextView" parent="Widget.AppCompat.AutoCompleteTextView">
        <item name="android:textColor">#FFF</item>
        <item name="android:textColorHint">#FFF</item>
    </style>
    <style name="recyclerViewStyle" parent="Widget.AppCompat.ListView">
        <item name="android:background">#EEEEB8</item>
    </style>
    <style name="cardViewStyle" parent="CardView.Light">
        <item name="android:textColor">#000</item>
        <item name="android:background">#FFFFC9</item>
    </style>
</resources>